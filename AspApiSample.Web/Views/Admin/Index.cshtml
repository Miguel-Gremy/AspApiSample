@model AspApiSample.Web.Models.Admin.IndexModel
@{
    ViewData["Title"] = "Index";
}

@if (Model.Messages is not null)
{
    <div class="alert alert-success" role="alert">
        @foreach (var message in Model.Messages)
        {
            <p>@message</p>
        }
    </div>
}

<a asp-area="" asp-controller="Admin" asp-action="Users">Users</a>
<a asp-area="" asp-controller="Admin" asp-action="Roles">Roles</a>